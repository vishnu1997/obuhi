{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowJs": true,
    "pretty": true,
    "sourceMap": true,
    "target": "es6",
    "resolveJsonModule": true,
    "outDir": "build/"
  },
  "include": [
    "next-env.d.ts",
    "./src/**/*",
    "./config"
  ],
  "exclude": [
    "node_modules"
  ]
}
